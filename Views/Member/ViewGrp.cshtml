@model IEnumerable<Ebiz.Models.GrpMst>
    @{
        ViewBag.Title = "ViewGrp";
        Layout = "~/Views/Shared/_Member.cshtml";
    }

   

        <table id="example" class="display" style="width:100%">
            <thead>
                <tr>
                    <th>Group ID</th>
                    <th>Name</th>
                    <th>Desc</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var res in Model)
            {
                    <tr>
                        <td>@res.Id</td>
                        <td>@res.Name</td>
                        <td>@res.Desc</td>
                    </tr>
                }
            </tbody>
        </table>

        @section reg{
            <script type="text/javascript">
                $(document).ready(function () {
                    $('#example').DataTable({
                        "order": [[0, "desc"]]
                    });
                });
            </script>
        }

